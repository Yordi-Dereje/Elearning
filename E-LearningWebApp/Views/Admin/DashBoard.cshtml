@{
    
    Layout = "~/Views/Shared/AdminLayout.cshtml";
    
   
}
<br />
<br />
<br/>
<br />
<br />

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>iofrm</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/content/iofrm-form/css/style.css">
    <link rel="stylesheet" href="~/content/iofrm-form/css/responsive.css">
</head>

<section id="counts" class="counts section-bg">
    <div class="container">

        <div class="row counters">

            <div class="col-lg-3 col-6 text-center">
                <span data-purecounter-start="0" data-purecounter-end="1232" data-purecounter-duration="1" class="purecounter"></span>
                <p>Students</p>
            </div>

            <div class="col-lg-3 col-6 text-center">
                <span data-purecounter-start="0" data-purecounter-end="64" data-purecounter-duration="1" class="purecounter">@ViewBag.ItemCountPayment</span>
                <p>Courses</p>
            </div>

            <div class="col-lg-3 col-6 text-center">
                <span data-purecounter-start="0" data-purecounter-end="@ViewBag.ItemCountCourse" data-purecounter-duration="1" class="purecounter"></span>
                <p>Course</p>
            </div>

            <div class="col-lg-3 col-6 text-center">
                <span data-purecounter-start="0" data-purecounter-end="@ViewBag.ItemCountSubCourse" data-purecounter-duration="1" class="purecounter"></span>
                <p>SubCourse</p>
            </div>

        </div>

    </div>
</section><!-- End Counts Section -->
<!-- Vendor JS Files -->
<script src="~/Content/assets/vendor/purecounter/purecounter_vanilla.js"></script>
<script src="~/Content/assets/vendor/aos/aos.js"></script>
<script src="~/Content/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/Content/assets/vendor/swiper/swiper-bundle.min.js"></script>
<script src="~/Content/assets/vendor/php-email-form/validate.js"></script>

<!-- Template Main JS File -->
<script src="~/Content/assets/js/main.js"></script>